{
  "expo": {
    "name": "comedorITSMIGRA",
    "slug": "comedorITSMIGRA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/comedor.png",
    "scheme": "comedoritsmigra",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "Comedor.ITSMIGRA",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/comedor.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/comedor.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/comedor.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permite a ComedorITSMIGRA acceder a tu cámara para escanear códigos QR."
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.20",
            "gradleChildProcessParams": [
              "-Xmx4g",
              "-XX:MaxMetaspaceSize=1g"
            ],
            "enableProGuard": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5e2fbdf0-25c3-4e63-bfd0-f4727dfde501"
      }
    }
  }
}